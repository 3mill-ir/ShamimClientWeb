@model ClientWeb.Models.DataModels.PageTitleDataModel
@{ var item = Model;}

<ol class="" style="direction:rtl;padding: 0;color: inherit;">
    @while (item != null)
    {
        if (item.Type == "DynamicPost")
        {
            <li><a href="@Url.Action("ListPost", "Post",new { profile = TempData["UserName"], lang = "FA", id = item.Id})">@item.Name</a></li>
        }
        else if (item.Type == "StaticPost")
        {
            <li><a href="@Url.Action("PageDetail", "Page",new { profile = TempData["UserName"], lang = "FA", id = item.Id})">@item.Name</a></li>
        }
        else if (item.Type == "NoneStaticDynamic")
        {
            <li><a href="#">@item.Name</a></li>
        }
        item = item.Menu2;
    }
</ol>
